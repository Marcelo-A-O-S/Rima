    "version": "0.2.0",
    "configurations": [
        {
            "name": "ts-node",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/Backend/src/index.ts",
            "sourceMaps": true,
            "args": [
                "${relativeFile}"
            ],
            "cwd": "${workspaceRoot}",
            "env": {
                "PORT":"4073",
                "USER_MYSQL":"root",
                "PASSWORD_MYSQL":"123456",
                "DATABASE_MYSQL":"DatabaseMysql"
            },
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}
